{
  "name": "basic-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make-forge-dev": "cross-env NODE_ENV=development electron-forge make",
    "make-forge-prod": "cross-env NODE_ENV=production electron-forge make",
    "make-forge-squirrel": "cross-env NODE_ENV=development electron-forge make --targets=@electron-forge/maker-squirrel",
    "make-forge-wix": "cross-env NODE_ENV=development electron-forge make --targets=@electron-forge/maker-wix",
    "publish-forge-save": "electron-forge publish --dry-run",
    "publish-forge": "electron-forge publish --from-dry-run",
    "make-builder": "electron-builder build --config=builder.config.js --publish=never",
    "publish-builder": "electron-builder --config=builder.config.js --publish=always",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Abdelrahman",
    "email": "are@luftborn.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/publisher-base": "^6.4.2",
    "dotenv": "^16.3.1",
    "electron-log": "^5.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-wix": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/publisher-electron-release-server": "^6.4.2",
    "cross-env": "^7.0.3",
    "electron": "25.3.2",
    "electron-builder": "^24.6.3"
  }
}
